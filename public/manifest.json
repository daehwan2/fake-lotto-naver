{
  "manifest_version": 3,
  "name": "Fake Lotto Naver",
  "description": "Fake Lotto Naver",
  "version": "1.0",
  "icons": {
    "16": "icons/logo.png",
    "32": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["https://search.naver.com/*"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_title": "Fake Lotto Naver",
    "default_icon": "icons/logo.png",
    "default_popup": "popup.html"
  }
}
