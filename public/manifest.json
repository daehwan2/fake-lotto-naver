{
  "manifest_version": 3,
  "name": "네이버 로또 번호 조작",
  "description": "네이버 로또 번호를 조작할 수 있습니다.",
  "version": "1.1.0",
  "icons": {
    "16": "icons/logo.png",
    "32": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["https://search.naver.com/*"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_title": "네이버 로또 번호 조작",
    "default_icon": "icons/logo.png",
    "default_popup": "popup.html"
  }
}
